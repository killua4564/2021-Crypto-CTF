from factordb.factordb import FactorDB
from gmpy2 import isqrt, next_prime
from Crypto.Util.number import *

a = 
b = 
c = 
flag = 

'''
a = 2*z**5 - x**3 + y*z
b = x**4 + y**5 + x*y*z
c = y**6 + 2*z**5 + z*y

c - a = x**3 + y**6 = (x + y**2)(x**2 - x*y**2 + y**4)
'''

factor = FactorDB(c-a)
factor.connect()
ca1, ca2 = factor.get_factor_list()

x = var("x")
assume(x, "integer")
sol = solve(x^2 - x*(ca1-x) + (ca1-x)**2 == ca2, x)

for x in map(int, sol):
    if x > ca1:
        continue

    y = int(isqrt(ca1 - x))
    if x + y**2 != ca1:
        continue

    z = (b - x**4 - y**5) // (x*y)
    if x**4 + y**5 + x*y*z != b:
        continue

    p = next_prime(x**2 + z**2 + y**2 << 76)
    q = next_prime(z**2 + y**3 - y*x*z ^^ 67)
    d = inverse_mod(31337, (p-1)*(q-1))
    print(long_to_bytes(pow(flag, d, p*q)).decode())
